TEMPLATE	= app
LANGUAGE	= C++

VPATH = @srcdir@
QMAKE_CXXFLAGS += -std=c++14 -I../ -DENABLE_XMLCONF
	       

DEFINES += MDU_INCLUDE_LOG=\'<libupnpp/log.h>\'

CONFIG	+= qt warn_on thread release debug
QT += widgets

LIBS += @UPMPDCLI_LIBS@

INCLUDEPATH += ../src
INCLUDEPATH += @srcdir@/../src
INCLUDEPATH += @srcdir@/..

HEADERS	+= confgui.h mainwindow.h

SOURCES	+= confmain.cpp \
           confgui.cpp \
           ../src/conftree.cpp \
           ../src/smallut.cpp \
           ../src/pathut.cpp

target.path = "$$PREFIX/bin"
INSTALLS += target
